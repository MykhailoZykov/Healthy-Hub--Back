              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorNotFoundResponse"
                }
              }
            }
          }
        }
      }
    },
    "/user/weight": {
      "post": {
        "tags": ["User"],
        "summary": "Create and update the user's current weight",
