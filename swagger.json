{
  "openapi": "3.0.1",
  "info": {
    "version": "1.0.0",
    "title": "Food_diary_Backend",
    "description": "web app about your calories"
  },
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "servers": [
    { "url": "https://food-diary-backend-kr1b.onrender.com/api" },
    { "url": "http://localhost:3000/api" }
  ],
  "tags": [
